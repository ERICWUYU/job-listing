<h2 class="text-center">提交简历</h2>
<div class="col-md-4 col-md-offset-4">
  <%= simple_form_for [@job,@resume] do |f| %>
  <div >
    <%= f.input :content %>

  </div>
  <%= f.label :attachment %>
  <%= f.file_field :attachment %>
  <%= f.submit "Submit", disable_with: "Submiting..." %>
  <% end %>
</div>
